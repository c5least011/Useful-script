local hint = Instance.new("Hint")  -- Tạo đối tượng Hint
hint.Text = "WELCOME!"  -- Nội dung thông báo
hint.Parent = game.Workspace  -- Đặt Hint vào Workspace

-- Sau một khoảng thời gian, xóa Hint
wait(5)  -- Đợi 5 giây
hint:Destroy()  -- Xóa Hint khỏi Workspace
